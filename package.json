{
  "name": "eve-cerebral",
  "productName": "Cerebral",
  "version": "0.7.1-dev",
  "description": "Character Manager for EVE Online",
  "author": "Prometheus Satyen",
  "license": "AGPL-3.0-only",
  "repository": "github:PrometheusSatyen/Cerebral",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "resources/icon.ico",
        "ignore": [
          ".idea"
        ]
      },
      "electronWinstallerConfig": {
        "name": "eve_cerebral",
        "icon": "resources/icon.ico",
        "setupIcon": "resources/icon.ico",
        "loadingGif": "resources/installing.gif"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "windowsStoreConfig": {
        "packageName": "",
        "name": "EVE Cerebral"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "material-ui": "^0.20.2",
    "react": "^16.7.0",
    "react-dnd": "^7.0.2",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^1.4.0",
    "react-sortable-tree": "^2.6.0",
    "react-table": "^6.8.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sanitize-html": "^1.20.0",
    "winston": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.3",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3"
  }
}
